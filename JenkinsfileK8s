pipeline {
  agent any
  stages {
    stage('Parse Input File') {
      steps {
        script {
          DOMAINS = readJSON file: 'domains.json'
          echo "domains: ${DOMAINS}"
          num_entries = DOMAINS.size()
          for (i=0; i < num_entries; i++) {
            echo DOMAINS[i].name
          }
        }
      }
    }
  }
}
