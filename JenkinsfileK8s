pipeline {
  agent any
  stages {
    stage('Parse Input File') {
      steps {
        script {
          DOMAINS = readJSON file: 'domains.json'
          for (i=0; i < DOMAINS.size(); i++) {
            echo DOMAINS[i].name
            build(
                job: 'gauntlt_tests',
                parameters: [
                    [
                        $class: 'StringParameterValue',
                        name: 'MYLIST',
                        value: MYLIST[element]
                    ]
                ]
            )
          }
        }
      }
    }
  }
}
